{
	"name": "@byeolnaerim/flex-layout",
	"version": "0.0.4",
	"type": "module",
	"license": "Apache-2.0",
	"author": "Joohyoung Kim (@oozu1994)",
	"description": "A React UI layout component for building resizable split views. Wrap panes with FlexLayout and drag the divider to resize horizontally or vertically.",
	"keywords": [
		"react",
		"ui",
		"layout",
		"flex",
		"split",
		"split-pane",
		"splitter",
		"resizable",
		"resize",
		"drag",
		"divider",
		"panel",
		"panes",
		"dock",
		"flex-layout",
		"flexLayout",
		"vscode",
		"rxjs",
		"typescript"
	],
	"homepage": "https://byeolnaerim.com/flex-layout",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/joohyoungkim19940805/flex-layout.git"
	},
	"bugs": {
		"url": "https://github.com/joohyoungkim19940805/flex-layout/issues"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./components": {
			"types": "./dist/components.d.ts",
			"import": "./dist/components.js",
			"require": "./dist/components.cjs"
		},
		"./hooks": {
			"types": "./dist/hooks.d.ts",
			"import": "./dist/hooks.js",
			"require": "./dist/hooks.cjs"
		},
		"./providers": {
			"types": "./dist/providers.d.ts",
			"import": "./dist/providers.js",
			"require": "./dist/providers.cjs"
		},
		"./utils": {
			"types": "./dist/utils.d.ts",
			"import": "./dist/utils.js",
			"require": "./dist/utils.cjs"
		},
		"./store": {
			"types": "./dist/store.d.ts",
			"import": "./dist/store.js",
			"require": "./dist/store.cjs"
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": [
		"**/*.css"
	],
	"scripts": {
		"build": "tsup && npm run copy:assets",
		"copy:assets": "cpy \"**/*.css\" \"../dist\" --cwd=src --parents",
		"dev": "tsup --watch",
		"prepack": "npm run build"
	},
	"devDependencies": {
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"cpy-cli": "^5",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"rxjs": "^7.8.2",
		"tsup": "^8.5.1",
		"typescript": "5.9.3"
	},
	"peerDependencies": {
		"react": ">=19",
		"react-dom": ">=19",
		"rxjs": ">=7"
	},
	"peerDependenciesMeta": {
		"rxjs": {
			"optional": true
		}
	},
	"dependencies": {
		"fast-deep-equal": "^3.1.3"
	},
	"typesVersions": {
		"*": {
			"components": [
				"dist/components.d.ts"
			],
			"hooks": [
				"dist/hooks.d.ts"
			],
			"providers": [
				"dist/providers.d.ts"
			],
			"utils": [
				"dist/utils.d.ts"
			],
			"store": [
				"dist/store.d.ts"
			]
		}
	}
}
